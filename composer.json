{
  "name": "novutec/whoisparser",
  "type": "library",
  "description": "Lookup domain names, IP addresses and AS numbers by WHOIS.",
  "keywords": ["whoisparser", "whois", "php"],
  "homepage": "https://github.com/3name/WhoisParser/",
  "author": "Novutec Inc.",
  "license": "Apache-2.0",
  "require": {
    "php": ">=7.1",
    "novutec/domainparser": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0"
  },
  "autoload": {
    "psr-4": { "Novutec\\WhoisParser\\": "src/" }
  },
  "autoload-dev": {
    "psr-4": { "Novutec\\WhoisParser\\Tests\\": "tests/"  }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
